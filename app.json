{
  "expo": {
    "name": "workouttracker",
    "slug": "workouttracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#121212"
      }
    },
    "plugins": ["expo-font"],
    "experiments": {
      "tsconfigPaths": true
    },
    "newArchEnabled": true
  }
}
